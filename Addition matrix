public class addmatrix {
    public static void main(String args[])
    {
        int a[][]={{1,1,1},{2,2,2},{3,3,3}};
        int b[][]={{1,1,1},{2,2,2},{3,3,3}};
        int res[][]=new int[3][3];
        for(int i=0;i<3;i++)
        {
            for(int j=0;j<3;j++)
            {
                res[i][j]=a[i][j]+b[i][j];
                System.out.print(res[i][j] + "  ");
            }
            System.out.println();

       }
       
    }

    }

  
