public class revnum {
    public static void main(String args[])
    {
        int num=12345;
        int rev=0;
        try{
            while(num>0)
            {
                int rem=num%10;
                rev=rev*10+rem;
                num/=10;
            }
            System.out.println(rev);

        }
        catch(Exception e)
        {
            System.out.println("invalid Datatype");
        }
    }
    
}
